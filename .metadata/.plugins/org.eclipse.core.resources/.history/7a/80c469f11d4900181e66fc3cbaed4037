package SA2;

public class WochentagTest {
public static void main(String[] args) {
    Wochentag wochentag = new Wochentag();
    System.out.println(wochentag.toString());
    
    for (int i = 0; i < 6; i++) {
        wochentag.naechsterTag();
        System.out.println(wochentag.toString());
        
    }
}
}
