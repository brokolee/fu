package adhoc3;

public class utilArrays {

    public static void main(String[] args) {
        int[] utilArray = new int[] {3, 10, 2, 47, 1, 4};
        java.util.Arrays.parallelSort(utilArray);
  
        for (int i = 0; i < utilArray.length; i++) {
            System.out.println(utilArray[i]);
        }

        
    }

}
