package EA2;

public class CarTest {
    public static void main(String[] args) {
        CarFactory factory = new CarFactory();
        RealCar myCar =  (RealCar) factory.giveMeACar();
        System.out.println(myCar.readMotorPowerInKiloWatt());
        myCar.tunePower();
        System.out.println(myCar.readMotorPowerInKiloWatt());
    }
}
